        };

        nav_layer {
            bindings = <
                &trans     &kp KP_N7 &kp KP_N8 &kp KP_N9  &kp SPC         &kp F11 &kp F12 &trans  &trans &trans
                &kp LNLCK  &kp KP_N4 &kp KP_N5 &kp KP_N6  &kp COMMA       &kp F1  &kp F2  &kp F3  &kp F4 &kp F5
