	  keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &none     &kp W  &kp F &kp P  &kp B         &kp J       &kp L     &kp U      &kp Y   &none
                &kp K_APP &kp S  &kp T &kp H  &kp K         &kp X       &kp N     &kp A      &kp I   &caps_word
                &kp R     &kp C  &kp G &kp D  &key_repeat   &key_repeat &kp M     &kp COMMA  &kp DOT &kp O
